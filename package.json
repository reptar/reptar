{
  "name": "yarnjs",
  "version": "1.4.0",
  "description": "spin your own yarn",
  "main": "./dist/lib/index.js",
  "scripts": {
    "compile": "babel lib --out-dir dist/lib && babel bin --out-dir dist/bin",
    "prepublish": "npm run compile && cp lib/config/defaults.yml dist/lib/config/defaults.yml",
    "test": "mocha --recursive --require ./test/setup.js --reporter spec",
    "test:unit": "npm test -- test/unit",
    "test:e2e": "npm test -- test/e2e",
    "lint": "eslint .",
    "coverage": "babel-node ./node_modules/.bin/isparta cover ./node_modules/mocha/bin/_mocha -- --recursive",
    "coveralls": "babel-node ./node_modules/.bin/isparta cover --report lcovonly ./node_modules/mocha/bin/_mocha -- --recursive && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "repository": "yarnjs/yarn",
  "keywords": [
    "blog",
    "blogging",
    "static",
    "static site",
    "static site generator"
  ],
  "author": "Harry Wolff <hello@hswolff.com> (http://hswolff.com/)",
  "license": "MIT",
  "engines": {
    "node": ">=4.0.0"
  },
  "preferGlobal": true,
  "dependencies": {
    "activity-logger": "^1.4.0",
    "babel-polyfill": "^6.16.0",
    "babel-runtime": "^6.11.6",
    "babelify": "^7.3.0",
    "bluebird": "^3.4.6",
    "browserify": "^13.1.0",
    "chalk": "^1.1.3",
    "chokidar": "^1.6.0",
    "cross-spawn": "^4.0.2",
    "find-up": "^2.0.0",
    "fs-extra": "^0.30.0",
    "glob": "^7.1.0",
    "gray-matter": "^2.0.2",
    "highlight.js": "^9.7.0",
    "home-or-tmp": "^2.0.0",
    "inquirer": "^1.1.3",
    "js-yaml": "^3.6.1",
    "less": "^2.7.1",
    "less-plugin-autoprefix": "^1.5.1",
    "less-plugin-clean-css": "^1.5.1",
    "less-plugin-npm-import": "yarnjs/less-plugin-npm-import#basedir",
    "live-server": "^1.1.0",
    "lodash": "^4.16.2",
    "moment": "^2.15.1",
    "node-sass": "^3.10.0",
    "nunjucks": "^2.5.2",
    "remarkable": "^1.7.0",
    "resolve": "^1.1.7",
    "rimraf": "^2.5.4",
    "slug": "^0.9.1",
    "uglifyify": "^3.0.3",
    "yargs": "^5.0.0",
    "yarn-excerpt": "0.7.0",
    "yarn-html-minifier": "^0.6.1",
    "yarn-scaffold": "^0.7.0"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-eslint": "^7.0.0",
    "babel-plugin-espower": "^2.3.1",
    "babel-plugin-lodash": "^3.2.9",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-class-properties": "^6.16.0",
    "babel-plugin-transform-es2015-destructuring": "^6.16.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.16.0",
    "babel-plugin-transform-es2015-parameters": "^6.16.0",
    "babel-plugin-transform-es2015-spread": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-plugin-transform-regenerator": "^6.16.1",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-register": "^6.16.0",
    "coveralls": "^2.11.14",
    "eslint": "^3.7.0",
    "isparta": "^4.0.0",
    "mocha": "^3.1.0",
    "mock-fs": "^3.11.0",
    "power-assert": "^1.4.1",
    "sinon": "^2.0.0-pre.2"
  }
}
